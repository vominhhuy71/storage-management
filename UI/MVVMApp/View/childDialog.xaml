<Window x:Class="MVVMApp.View.childDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MVVMApp.View" xmlns:vm="clr-namespace:MVVMApp.ViewModel"
        mc:Ignorable="d"
        Title="childDialog" Height="150" Width="300">
    <Window.DataContext>
        <vm:ItemInfoViewModel></vm:ItemInfoViewModel>
    </Window.DataContext>
    <DockPanel LastChildFill="True">
        <Label FontSize ="30">Saved!</Label>
        <Label FontSize="15" Content="{Binding Info, FallbackValue='Your data was saved!'}" Margin="0,0,0,-0.2" Width="152" Height="64" VerticalAlignment="Bottom"></Label>
    </DockPanel>
</Window>
